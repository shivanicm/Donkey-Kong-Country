module ColorTransform3( input [3:0] in,
							  inout [15:0] out
							 );
							 


	always @(in) // check 
	case(in)
		4'h0 :
			out = 16'b1111100000000000;
		4'h1 :
			out = 16'b0000100010000011;
		4'h2 :
			out = 16'b0000000010100000;
		4'h3 :
			out = 16'b0010000011100000;
		4'h4 :
			out = 16'b0001100011100001;
		4'h5 :
			out = 16'b 0100100100100000;
		4'h6 :
			out = 16'b1010000111100000;
		4'h7 :
			out = 16'b0011100101100000;
		4'h8 :
			out = 16'b0001100111100000;
		4'h9 :
			out = 16'b0011101110100000;
		4'hA :
			out = 16'b0010010110100000;
		4'hB :
			out = 16'b0100000000100010;
		default :
			out = 16'b1111100000000000;

	endcase
	
	endmodule
	